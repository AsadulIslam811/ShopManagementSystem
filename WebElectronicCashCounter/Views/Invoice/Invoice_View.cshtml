@model IEnumerable<WebElectronicCashCounter.Models.Invoice>

@{
    ViewBag.Title = "Invoice View.";
}

<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>
<p>
    @Html.ActionLink("Add New Invoice", "Invoice_Add")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.ProductId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ValuePerProduct)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PricePerProduct)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EntryDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExpireDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MenupactureCompany)
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProductId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ValuePerProduct)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PricePerProduct)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EntryDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExpireDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MenupactureCompany)
        </td>
    
    </tr>
}

</table>
